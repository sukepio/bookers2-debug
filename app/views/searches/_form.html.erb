<% if user_signed_in? %>
  <%= form_with url: search_path, method: :get do |f| %>
    <%= f.text_field 'content' %>
    <%= select_tag 'model', options_for_select({ "User" => "user", "Book" => "book"}) %>
    <%= select_tag 'method', options_for_select({ "完全一致" => "perfect", "前方一致" => "前方一致", "後方一致" => "後方一致", "部分一致" => "partial"}) %>
    <%= f.submit '検索' %>
  <% end %>
<% end %>